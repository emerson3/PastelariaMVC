@model UsuarioViewModel;

@{
    Layout = null;
    ViewData["Title"] = "Cadastrar tarefa";
}

<link rel="stylesheet" type="text/css" href="~/css/Cadastrar/cadastrar.css" />
<div class="conteudo">
<div class="parte-superior">
    <img src="~/img/right-arrow.png" />
    <h3>@ViewData["Title"]</h3>
</div>
    <div class="conteudo-esquerda">
        <form id="form-cadastrar" onsubmit="usuario.cadastrar()">
            <h3>Título</h3>
            <input type="text" />
            <h3>Descrição</h3>
            <input type="textarea" class="input-descricao" />
        </form>
    </div>
    
    <div class="conteudo-direita">
        <h3>Funcionários</h3>
        <div class="funcionarios">
            @foreach (var item in Model.Usuarios)
            {
                <li>@item.<a href=""><img src="~/img/more.png" /></a></li>
            }
        </div>
    </div>

</div>
